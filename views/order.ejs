<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Order List</title>
</head>
<body>
    <h2>Order List for Seller: -</h2>
    <p>Seller ID: <%= order.seller.seller_id %></p>
    <p>Seller name: <%= order.seller.name %></p>

    <h4>List of Items</h4>
        <ul>
            <% order.items.forEach(function(item) { %>
                <li><%= item.name %></li>
            <% }); %>
        </ul>
</body>
</html>

